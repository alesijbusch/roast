        .page-top
            +breadcrumbs({
                content2: 'Компания',
                href_content2: '',
                content3: ''
            })
            .page-top__header
                h1.page-top__title Компания
    .container
        +tabs({
            list: [
                {
                    name: 'О компании',

                },
                {name: 'Команда', },
                {name: 'Отзывы',mod: 'active'},
                {name: 'Контакты'},
            ]
